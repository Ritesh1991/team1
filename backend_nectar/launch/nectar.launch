<launch>
    <node name="$(anon nav_server)" pkg="map_annotator" type="ServingServerNode" respawn="true"/>
    <node name="$(anon serving_server" pkg="perception" tpye="NavigationServerNode" respawn="true"/>
    <node name="nectar" pkg="backend_nectar" type="backend.py" required="true" /> 
    <node name="$(anon perception_demo)" pkg="perception" type="point_cloud_demo" args="cloud_in:=/head_camera/depth_registered/points"/>
    <param name="ec_cluster_tolerance" value="0.03" />
    <param name="ec_min_cluster_size" value="60" />
    <param name="ec_max_cluster_size" value="400" />
    <param name="distance_above_plane" value="0.01" />
    <param name="crop_min_x" value="-0.5" />
    <param name="crop_max_z" value="1.5" />
    <param name="crop_max_x" value="1.05" />
    <param name="crop_min_z" value="0.7" />
</launch>